{
    "name": "morebec/orkestra-modeling",
    "description": "Orkestra component providing classes and utilities for DDD modeling",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jwillp",
            "email": "jwilliam.perreault@morebec.com"
        }
    ],
    "suggest": {
        "morebec/orkestra-postgresql-document-store": "PostgreSQL implementation a document store to easily persist Aggregates without an ORM"
    },
    "require": {
        "php": ">=7.3",
        "morebec/orkestra-collections": "^2.0.9",
        "morebec/orkestra-messaging": "^2.0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "ext-dom": "*",
        "phpstan/phpstan": "^0.12.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Morebec\\Orkestra\\Modeling\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Morebec\\Orkestra\\Modeling\\": "tests"
        }
    }
}
