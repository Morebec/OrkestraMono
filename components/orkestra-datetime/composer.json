{
    "name": "morebec/orkestra-datetime",
    "description": "Extension of CakePHP/Chronos that adds utility functions around DateTime manipulation.",
    "require": {
        "php": ">=7.3",
        "cakephp/chronos": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "ext-dom": "*",
        "phpstan/phpstan": "^0.12.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Morebec\\Orkestra\\DateTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Morebec\\Orkestra\\DateTime\\": "tests"
        }
    }
}
